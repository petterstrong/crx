{
  "manifest_version": 2,
  "name": "Lesson-Two",
  "description": "例2：一周天气",
  "version": "1.0",
  "browser_action": {
    "default_icon": "/images/icon19.png",
    "default_popup": "index.html",
    "default_title": "点击查看天气"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.sojson.com/open/api/weather/json.shtml?city=%E6%9D%AD%E5%B7%9E"
  ],
  "options_page": "settings.html"
}
